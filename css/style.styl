/* css transitions */
#ra-container
  transition transform .25s
  -webkit-transition -webkit-transform .25s
.ra-large #ra-main
  transition left .25s, max-width .25s
  -webkit-transition left .25s, max-width .25s
#ra-nav
  transition max-height .25s
  -webkit-transition max-height .25s
